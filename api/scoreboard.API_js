{"GET":"var scoreboardSQL='SELECT id,nickname,score FROM scoreboard ORDER BY score DESC;';\nvar scoreboard=query(scoreboardSQL);\nwrite('scoreboard',scoreboard);","POST":"var score=param('score');\nvar nick=param('nick');\n\nvar params={};\nparams.score=score;\nparams.nick=nick;\n\n\nvar scoreInsertSQL='INSERT INTO scoreboard(nickname,score) VALUES(:nick, :score);';\nvar scoreInsert=query(scoreInsertSQL, params);\nwrite('scoreInsert', scoreInsert);"}